project DataView_c is
   for Languages use ("ASN1");
   for Source_Dirs use (".");
   for Source_Files use ("dataview-uniq.asn");
   for Object_Dir use "code";

   package Naming is
       for Body_Suffix ("ASN1") use ".asn";
   end Naming;

   package Compiler is
       for Driver ("ASN1") use "mono";

       for Leading_Required_Switches ("ASN1") use
         (external("ASN1SCC"),
         "-c",
          "-typePrefix",
          "Asn1Scc");
   end Compiler;
end DataView_c;